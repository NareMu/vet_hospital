          "data": "{\n  \"aggregate\": \"CLIENT\",\n  \"pipeline\": [\n    { \"$unwind\": \"$pets\" },\n    { \"$unwind\": \"$pets.medical_record\" },\n    { \"$unwind\": \"$pets.medical_record.billing\" },\n    { \"$match\": { \"pets.medical_record.billing.payment_status\": \"Paid\" } },\n    { \n      \"$group\": {\n        \"_id\": \"$pets.medical_record.crew_id\",\n        \"total_revenue\": { \"$sum\": \"$pets.medical_record.billing.total\" }\n      }\n    },\n    { \"$sort\": { \"total_revenue\": -1 } },\n    { \n      \"$lookup\": {\n        \"from\": \"CREW\",\n        \"localField\": \"_id\",\n        \"foreignField\": \"crew_id\",\n        \"as\": \"crew_details\"\n      }\n    },\n    { \"$unwind\": \"$crew_details\" },\n    { \n      \"$project\": { \n        \"_id\": 0,\n        \"crew_id\": \"$_id\",\n        \"crew_name\": { \"$concat\": [\"$crew_details.first_name\", \" \", \"$crew_details.last_name\"] },\n        \"position\": \"$crew_details.position\",\n        \"total_revenue\": \"$total_revenue\"\n      }\n    }\n  ],\n  \"cursor\": {}\n}\n"
